{{ define "yield" }}
<a href="/problems/new">New Problem</a>
<h1>Problems</h1>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
  {{ range .Data }}
    <tr>
      <td>{{.ID}}</td>
      <td><a href="/problems/{{.ID}}">{{.Name}}</a></td>
    </tr>
  {{ end }}
  </tbody>
</table>
{{ end }}
